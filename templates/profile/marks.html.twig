{% extends 'base.html.twig' %}

{% block body %}
    <table class="table table-stripped">
        <thead>
            <tr>
                <td>Mark</td>
            </tr>
        </thead>
        <tbody>
            {% for mark in marks %}
                <tr>
                    <td>{{ mark.value }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}